<?xml version="1.0" encoding="UTF-8" ?>
<phpdocumentor>
    <title>Viadutoo</title>
    <parser>
        <default-package-name>Default</default-package-name>
        <encoding>utf-8</encoding>
        <target>phpdoc-cache</target>
        <markers>
            <item>TODO</item>
            <item>FIXME</item>
        </markers>
        <extensions>
            <extension>php</extension>
        </extensions>
        <visibility>public,protected</visibility>
    </parser>
    <files>
        <directory>lib/Viadutoo</directory>
        <ignore-hidden>true</ignore-hidden>
        <ignore-symlinks>true</ignore-symlinks>
    </files>
    <transformer>
        <target>doc-build</target>
    </transformer>
    <logging>
        <level>error</level>
    </logging>
    <transformations>
        <template name="clean"/>
    </transformations>
    <translator>
        <locale>en</locale>
    </translator>
</phpdocumentor>
